{
    "manifest_version": 3,
    "name": "timetab",
    "short_name": "timetab",
    "description": "New tab page",
    "version": "0.1.0",
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "action": {
        "default_icon": {
            "16": "timetab16.png",
            "32": "timetab32.png",
            "48": "timetab48.png",
            "128": "timetab128.png"
        }
    },
    "icons" : {
        "16": "timetab16.png",
        "32": "timetab32.png",
        "48": "timetab48.png",
        "128": "timetab128.png"
    },
    "background": {
        "service_worker": "service_workers.js"
    },
    "permissions": [
        "geolocation"
    ]
}